{
  "name": "hithouse-test-automation",
  "version": "1.0.0",
  "description": "Profesionalno automatizovano testiranje web aplikacije hithouse.ba",
  "main": "index.js",
  "scripts": {
    "test": "node run-tests.js",
    "test:html": "mocha tests/**/*.test.js --timeout 120000 --reporter mochawesome",
    "test:search": "mocha tests/search/*.test.js --timeout 120000 --reporter spec",
    "report": "node scripts/generate-report.js"
  },
  "keywords": [
    "selenium",
    "mocha",
    "test-automation",
    "hithouse",
    "qa",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "selenium-webdriver": "^4.15.0"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0"
  }
}